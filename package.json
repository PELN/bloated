{
  "name": "bloated",
  "version": "1.0.0",
  "description": "Fork this repo, then clone it to your computer",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass source/css/style.scss source/css/style.css",
    "concat-css": "cat source/css/*.css > build/css/all.css",
    "autoprefix": "postcss build/css/all.css --use autoprefixer -d build/css/prefix",
    "minifycss": "postcss build/css/all.css --use cssnano -d build/css/min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PELN/bloated.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PELN/bloated/issues"
  },
  "homepage": "https://github.com/PELN/bloated#readme",
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "cssnano": "^3.10.0",
    "node-sass": "^4.9.0",
    "postcss-cli": "^5.0.0"
  }
}
